#include <iostream>
#include <cmath>
using namespace std;

bool isprimeNumber(int n, int i) {
if (n <= 1) {
return false;
}
for (int j = 2; j <= sqrt(n); j++) {
if (n % j == 0) {
return false;
}
}
return true;
}

